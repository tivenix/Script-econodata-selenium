def main():
    aba = cc.chrome.open("https://www.econodata.com.br/consulta-empresa/")
    ui(locator.chrome.barrapesquisa).set_text('97.546.023/0001-38')
    ui(locator.chrome.button_icone_buscar_pesquisar).click()
    ui(locator.chrome.paginaempresa).click()
    print(aba.find_element_by_xpath("collapse-dados-receita").get_text)
    #aba.find_element(locator.chrome.botaopesquisa).click()
    
    #aba.find_element(locator.chrome.barrapesquisa).clear_text
    aba


if __name__ == "__main__":
    main()
